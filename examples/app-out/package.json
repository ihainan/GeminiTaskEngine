{
  "name": "fastercap-desktop",
  "version": "1.0.0",
  "description": "FasterCap Desktop - 3D Capacitance Calculation Tool",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --x64",
    "build:win": "electron-builder --win --x64"
  },
  "author": "AppSynth",
  "license": "MIT",
  "devDependencies": {
    "electron": "23.3.13",
    "electron-builder": "24.6.4"
  },
  "dependencies": {
    "three": "0.155.0"
  },
  "build": {
    "appId": "com.appsynth.fastercap",
    "productName": "FasterCap Desktop",
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "target": {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      }
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}